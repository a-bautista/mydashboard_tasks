{
  "AWSEBDockerrunVersion": 2,
  "containerDefinitions": [
    {
      "name": "web",
      "image": "alexbr9007/web",
      "hostname": "web",
      "essential": false,
      "memory": 128
    },
    {
      "name": "db",
      "image": "alexbr9007/db",
      "hostname": "db",
      "essential": false,
      "memory": 128
    },
    {
      "name": "nginx",
      "image": "alexbr9007/nginx",
      "hostname": "nginx",
      "essential": true,
      "portMappings": [
        {
          "hostPort": 80,
          "containerPort": 8888
        }
      ],
      "memory": 128
    }
  ]
}
